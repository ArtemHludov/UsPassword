using System;

namespace llessons_3
{
    class Program
    {
        static void Main(string[] args)
        {


            Console.WriteLine("ведите пароль");


            for (int i = 0; i < 4; i++)
            {
                int a = Convert.ToInt32(Console.ReadLine());

                if (a != 2306)
                {
                    Console.WriteLine("Пароль не верный");
                }
                else
                {
                    Console.WriteLine("UserIdentification");
                    break;
                }

            }


        }
    }
}
